{
  "name": "annos-webapp",
  "version": "0.1.0",
  "private": true,
  "manifest": {
    "name": "Annos Browser Extension",
    "permissions": [
      "activeTab"
    ],
    "key": ""
  },
  "scripts": {
    "dev": "PORT=4000 next dev --turbopack",
    "dev:prod": "NODE_ENV=production PORT=4000 next dev",
    "dev:extension": "plasmo dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "build:extension": "NODE_ENV=production plasmo build",
    "package:extension": "plasmo package",
    "start": "PORT=4000 next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mantine/core": "^8.1.3",
    "@mantine/dates": "^8.1.3",
    "@mantine/dropzone": "^8.1.3",
    "@mantine/form": "^8.1.3",
    "@mantine/hooks": "^8.1.3",
    "@mantine/notifications": "^8.1.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "next": "15.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.332",
    "@types/node": "^20.10.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "15.4.1",
    "plasmo": "^0.90.5",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  }
}
